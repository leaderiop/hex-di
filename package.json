{
  "name": "hex-di",
  "version": "0.0.0",
  "private": true,
  "description": "HexDI - Hexagonal Dependency Injection for TypeScript",
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:types": "vitest typecheck",
    "typecheck": "tsc --noEmit",
    "clean": "pnpm -r clean"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.15.0"
}
